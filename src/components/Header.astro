---
import Chrome from "./icons/Chrome.astro";
import Facebook from "./icons/Facebook.astro";
import Twitter from "./icons/Twitter.astro";
import Instragam from "./icons/Instragam.astro";
import Search from "./icons/Search.astro";
---
<div class="bg-black/80 text-white/90 w-full h-5 flex justify-between ">
    <div class="flex ml-20 space-x-4 ">
        <Twitter class="size-3"/>
        <Instragam class="size-3"/>
        <Facebook class="size-3"/>
        <Chrome class="size-3"/>
    </div>
    <div class="items-center justify-center">
        <p class="text-sm ">TEXT</p>
    </div>
    <div class="flex mr-20 space-x-4">
        <Search class="size-3"/>
        <Chrome class="size-3"/>
    </div>
</div>
<div class="flex space-x-8 justify-center w-full h-10 bg-white font-semibold text-lg my-2 text-gray-800">
    <a>Home</a>
    <a>Pages</a>
    <a>Portafolio</a>
    <a class="font-extrabold text-xl">LITHO</a>
    <a>Elements</a>
    <a>Features</a>
    <a>Blog</a>
</div>